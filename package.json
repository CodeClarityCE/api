{
  "name": "api",
  "version": "0.0.43-alpha",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest build && ENV=prod node ./dist/utils/validate-env.js && ENV=prod nest start",
    "start:dev": "nest build && ENV=dev node ./dist/utils/validate-env.js && ENV=dev nest start --watch",
    "start:staging": "nest build && ENV=staging node ./dist/utils/validate-env.js && ENV=staging nest start",
    "start:test": "nest build && ENV=test node ./dist/utils/validate-env.js && ENV=test nest start --watch",
    "start:debug": "nest build && ENV=dev node ./dist/utils/validate-env.js && ENV=dev nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "nest build && ENV=prod node dist/scripts/run-migrations.js && ENV=prod node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "type-check": "tsc --noEmit",
    "check": "yarn lint && yarn type-check && yarn format && yarn test && yarn test:e2e test/integration/*contract*.e2e-spec.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:generate:codeclarity": "typeorm-ts-node-commonjs migration:generate src/migrations/codeclarity/Init -d src/datasources/codeclarity.datasource.ts",
    "migration:generate:knowledge": "typeorm-ts-node-commonjs migration:generate src/migrations/knowledge/Init -d src/datasources/knowledge.datasource.ts",
    "migration:generate:plugins": "typeorm-ts-node-commonjs migration:generate src/migrations/plugins/Init -d src/datasources/plugins.datasource.ts",
    "migration:run:codeclarity": "ts-node -r tsconfig-paths/register src/scripts/run-migration-cli.ts codeclarity",
    "migration:run:knowledge": "ts-node -r tsconfig-paths/register src/scripts/run-migration-cli.ts knowledge",
    "migration:run:plugins": "ts-node -r tsconfig-paths/register src/scripts/run-migration-cli.ts plugins",
    "migration:revert:codeclarity": "typeorm-ts-node-commonjs migration:revert -d src/datasources/codeclarity.datasource.ts",
    "migration:revert:knowledge": "typeorm-ts-node-commonjs migration:revert -d src/datasources/knowledge.datasource.ts",
    "migration:revert:plugins": "typeorm-ts-node-commonjs migration:revert -d src/datasources/plugins.datasource.ts"
  },
  "dependencies": {
    "@css-inline/css-inline": "0.18.0",
    "@fastify/compress": "8.3.1",
    "@fastify/multipart": "9.3.0",
    "@fastify/static": "8.3.0",
    "@nestjs-modules/mailer": "2.0.2",
    "@nestjs/axios": "4.0.1",
    "@nestjs/common": "11.1.10",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.1.10",
    "@nestjs/jwt": "11.0.2",
    "@nestjs/passport": "11.0.5",
    "@nestjs/platform-fastify": "11.1.10",
    "@nestjs/platform-socket.io": "11.1.10",
    "@nestjs/swagger": "11.2.3",
    "@nestjs/typeorm": "11.0.0",
    "@nestjs/websockets": "11.1.10",
    "@webundsoehne/nest-fastify-file-upload": "2.3.1",
    "amqplib": "0.10.9",
    "axios": "1.13.2",
    "bcrypt": "6.0.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.3",
    "dotenv": "17.2.3",
    "fastify": "5.6.2",
    "handlebars": "4.7.8",
    "jsonwebtoken": "9.0.3",
    "lodash": "4.17.21",
    "ms": "2.1.3",
    "nodemailer": "7.0.12",
    "octokit": "5.0.5",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "pg": "8.16.3",
    "prom-client": "15.1.3",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.2",
    "semver": "7.7.3",
    "socket.io": "4.8.3",
    "tslib": "2.8.1",
    "typeorm": "0.3.28"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@nestjs/cli": "11.0.14",
    "@nestjs/schematics": "11.0.9",
    "@nestjs/testing": "11.1.10",
    "@octokit/types": "16.0.0",
    "@types/amqplib": "0.10.8",
    "@types/bcrypt": "6.0.0",
    "@types/eslint-plugin-security": "3.0.0",
    "@types/express": "5.0.6",
    "@types/jest": "30.0.0",
    "@types/jsonwebtoken": "9.0.10",
    "@types/ms": "2.1.0",
    "@types/node": "25.0.3",
    "@types/nodemailer": "7.0.4",
    "@types/passport-jwt": "4.0.1",
    "@types/pg": "8.16.0",
    "@types/semver": "7.7.1",
    "@typescript-eslint/parser": "8.50.1",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-security": "3.0.1",
    "globals": "16.5.0",
    "jest": "30.2.0",
    "jest-util": "30.2.0",
    "prettier": "3.7.4",
    "source-map-support": "0.5.21",
    "ts-jest": "29.4.6",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.50.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/*.spec.ts",
      "!**/*.e2e-spec.ts",
      "!**/node_modules/**",
      "!**/dist/**"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/../test/utils/jest-setup.ts"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    },
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  },
  "packageManager": "yarn@4.12.0"
}
