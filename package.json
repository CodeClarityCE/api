{
  "name": "api",
  "version": "0.0.31-alpha",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest build && ENV=prod node ./dist/src/utils/validate-env.js && ENV=prod nest start",
    "start:dev": "nest build && ENV=dev node ./dist/src/utils/validate-env.js && ENV=dev nest start --watch",
    "start:staging": "nest build && ENV=staging node ./dist/src/utils/validate-env.js && ENV=staging nest start",
    "start:test": "nest build && ENV=test node ./dist/src/utils/validate-env.js && ENV=test nest start --watch",
    "start:debug": "nest build && ENV=dev node ./dist/src/utils/validate-env.js && ENV=dev nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "nest build && ENV=prod node ./dist/src/utils/validate-env.js && ENV=prod node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typecheck": "tsc --noEmit",
    "check": "yarn lint && yarn typecheck && yarn format && yarn test && yarn test:e2e test/integration/*contract*.e2e-spec.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@fastify/compress": "8.1.0",
    "@fastify/multipart": "9.0.3",
    "@fastify/static": "8.2.0",
    "@nestjs-modules/mailer": "2.0.2",
    "@nestjs/axios": "4.0.0",
    "@nestjs/common": "11.1.3",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.1.3",
    "@nestjs/jwt": "11.0.0",
    "@nestjs/passport": "11.0.5",
    "@nestjs/platform-fastify": "11.1.3",
    "@nestjs/platform-socket.io": "11.1.3",
    "@nestjs/swagger": "11.2.0",
    "@nestjs/typeorm": "11.0.0",
    "@nestjs/websockets": "11.1.3",
    "@webundsoehne/nest-fastify-file-upload": "2.3.1",
    "amqplib": "0.10.8",
    "axios": "1.10.0",
    "bcrypt": "6.0.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.2",
    "cvss-parser": "file:cvss-parser-js/",
    "dotenv": "17.0.1",
    "fastify": "5.3.3",
    "handlebars": "4.7.8",
    "jsonwebtoken": "9.0.2",
    "lodash": "4.17.21",
    "ms": "2.1.3",
    "nodemailer": "7.0.4",
    "octokit": "5.0.3",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "pg": "8.16.3",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.2",
    "semver": "7.7.2",
    "socket.io": "4.8.1",
    "tslib": "2.8.1",
    "typeorm": "0.3.25",
    "uuid": "11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "9.30.1",
    "@nestjs/cli": "11.0.7",
    "@nestjs/schematics": "11.0.5",
    "@nestjs/testing": "11.1.3",
    "@octokit/types": "14.1.0",
    "@rushstack/eslint-patch": "1.12.0",
    "@types/amqplib": "0.10.7",
    "@types/bcrypt": "5.0.2",
    "@types/cvss-parser": "file:cvss-parser-js/",
    "@types/express": "5.0.3",
    "@types/jest": "30.0.0",
    "@types/jsonwebtoken": "9.0.10",
    "@types/ms": "2.1.0",
    "@types/node": "24.0.10",
    "@types/nodemailer": "6.4.17",
    "@types/passport-jwt": "4.0.1",
    "@types/pg": "8.15.4",
    "@types/semver": "7.7.0",
    "@types/supertest": "6.0.3",
    "@types/uuid": "10.0.0",
    "eslint": "9.30.1",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-cypress": "5.1.0",
    "globals": "16.3.0",
    "jest": "30.0.4",
    "jest-util": "30.0.2",
    "prettier": "3.6.2",
    "source-map-support": "0.5.21",
    "supertest": "7.1.1",
    "ts-jest": "29.4.0",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.35.1",
    "webpack": "5.99.9"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/*.spec.ts",
      "!**/*.e2e-spec.ts",
      "!**/node_modules/**",
      "!**/dist/**"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/../test/utils/jest-setup.ts"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    },
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  },
  "packageManager": "yarn@4.9.2"
}
